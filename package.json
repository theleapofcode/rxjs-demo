{
  "name": "rxjs-demo",
  "version": "1.0.0",
  "description": "Demo for RxJS",
  "scripts": {
    "srcserver:start": "babel-node buildScripts/srcServer.js",
    "start": "npm-run-all --parallel srcserver:start lint:watch test:watch mockapi:start",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel start localtunnel",
    "lint": "esw webpack.config.*, src, buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "mockdata:generate": "babel-node buildScripts/generateMockData.js",
    "premockapi:start": "npm run mockdata:generate",
    "mockapi:start": "json-server --watch src/api/db.json --port 3001",
    "dist:clean": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all dist:clean test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theleapofcode/rxjs-demo.git"
  },
  "keywords": [
    "rxjs"
  ],
  "author": "theleapofcode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theleapofcode/rxjs-demo/issues"
  },
  "homepage": "https://github.com/theleapofcode/rxjs-demo#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.9.1",
    "json-schema-faker": "^0.4.0",
    "json-server": "^0.9.4",
    "localtunnel": "^1.8.2",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "open": "0.0.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "voca": "^1.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "rxjs": "^5.0.3"
  }
}
